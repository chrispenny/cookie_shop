<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- Uncomment this if you have PEAR Log installed
	  <log>
		<type>file</type>
		<name>/path/to/propel.log</name>
		<ident>propel-ci</ident>
		<level>7</level>
	  </log>
	  -->
	<propel>
		<datasources default="ci">
			<datasource id="ci">
				<adapter>mysql</adapter> <!-- sqlite, mysql, mssql, oracle, or pgsql -->
				<connection>
					<dsn>mysql:host=host;dbname=db</dsn>
					<user>user</user>
					<password>pass</password>
				</connection>
			</datasource>
		</datasources>
	</propel>
</config>
